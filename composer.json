{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "2.10.4",
        "fideloper/proxy": "4.4.0",
        "laravel/framework": "7.28.1",
        "laravel/helpers": "1.3.0",
        "laravel/tinker": "2.4.2",
        "laravelcollective/html": "6.2.0",
        "willvincent/feeds": "2.1.0",
        "laravel/ui": "2.4.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.8.1",
        "filp/whoops": "2.7.3",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "5.0.2",
        "nunomaduro/larastan": "0.6.4",
        "phpstan/phpstan": "0.12.42",
        "phpunit/php-code-coverage": "9.1.11",
        "phpunit/phpunit": "9.3.10"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
