{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "2.12.0",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "2.0.3",
        "guzzlehttp/guzzle": "7.2.0",
        "laravel/framework": "8.13.0",
        "laravel/helpers": "1.4.0",
        "laravel/tinker": "2.5.0",
        "laravelcollective/html": "6.2.0",
        "willvincent/feeds": "2.2.0",
        "laravel/ui": "3.1.0"
    },
    "require-dev": {
        "facade/ignition": "2.5.0",
        "barryvdh/laravel-ide-helper": "2.8.1",
        "filp/whoops": "2.9.1",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "5.1.0",
        "nunomaduro/larastan": "0.6.9",
        "phpstan/phpstan": "0.12.54",
        "phpunit/php-code-coverage": "9.2.3",
        "phpunit/phpunit": "9.4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
