{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "2.0.5",
        "guzzlehttp/guzzle": "7.4.1",
        "laravel/framework": "8.80.0",
        "laravel/tinker": "2.6.3",
        "laravelcollective/html": "6.2.1",
        "willvincent/feeds": "2.2.0",
        "laravel/ui": "3.4.1"
    },
    "require-dev": {
        "facade/ignition": "2.17.4",
        "barryvdh/laravel-ide-helper": "2.11.0",
        "fakerphp/faker": "1.17.0",
        "mockery/mockery": "1.4.4",
        "nunomaduro/collision": "5.11.0",
        "nunomaduro/larastan": "1.0.2",
        "phpstan/phpstan": "1.4.2",
        "phpunit/php-code-coverage": "9.2.10",
        "phpunit/phpunit": "9.5.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
