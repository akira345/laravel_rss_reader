{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "2.9.2",
        "fideloper/proxy": "4.2.1",
        "laravel/framework": "6.0.4",
        "laravel/helpers": "^1.1",
        "laravel/tinker": "1.0.10",
        "laravelcollective/html": "6.0.0",
        "willvincent/feeds": "2.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.6.5",
        "filp/whoops": "2.5.0",
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.3",
        "nunomaduro/collision": "3.0.1",
        "phpunit/phpunit": "8.3.5",
        "phpunit/php-code-coverage": "7.0.8"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
